                                "objectType": "Email"
                            }
                        ],
                        "firstName": "Czutzimir",
                        "lastName": "Young",
                        "transactionRelationship":{
                            "sendMoneyRelationship":{
                                "type":"Personal",
                                "displayText":"Personal",
                                "description":"Personal",
                                "objectType":"SendMoneyType"
                            },
                            "objectType":"TransactionRelationship"
                        },
                        "source": "PayPal",
                        "objectType": "Contact"
                    },
                    {
                        "displayInitials": "SH",
                        "registered": true,
                        "emails": [
                            {
                                "uniqueId": "sherlock-holmes@detective.com",
                                "emailAddress": "sherlock-holmes@detective.com",
