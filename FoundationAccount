                  "objectType": "Photo"
                },
                "emails": [
                  {
                    "objectType": "Email",
                    "uniqueId": "email111",
                    "schemaVersion": "v0.1",
                    "emailAddress": "testemail@paypal.com",
                    "confirmed": true,
                    "primary": true
                  }
                ],
                "phones": [
                    {
                        "profileItemType": "Phone",
                        "uniqueId": "73J4GV79GS6MQ",
                        "phoneNumber": "7182811223",
                        "phoneType": {
                            "uniqueId": "MOBILE",
